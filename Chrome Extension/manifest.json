{
  "manifest_version": 3,
  "name": "Simple AI Detector",
  "version": "1.0",
  "description": "A simple Chrome extension for AI detection.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "QuackScanAILogo.png",
      "48": "QuackScanAILogo.png",
      "128": "QuackScanAILogo.png"
    }
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}